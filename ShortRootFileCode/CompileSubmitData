
for which in Tuples_2011_Prompt_2;do
cat myCuts_halo.C > myCuts_new.C
sed -e '472 c\  TString location = "dcap://cmsgridftp.fnal.gov:24125";' \
-e '473 c\  TString path = "/pnfs/cms/WAX/11/store/user/lpcgg/askew/42Mono/'$which'/";' \
-e '474 c\  TString short_path ="/pnfs/fnal.gov/usr/cms/WAX/11/store/user/lpcgg/askew/42Mono/'$which'/";' \
-e '5149c\  outfile = new TFile("/uscmst1b_scratch/lpc1/3DayLifetime/sandhya/423_ShortRootFiles/'$which'.root","RECREATE");' \
-e '1475 c\  /*' \
-e '1479 c\  */' \
-e '1528 c\     scraping_isScrapingEvent = Scraping_isScrapingEvent;' \
-e '1612 c\  /*' \
-e '1638 c\  */'  < myCuts_new.C > myCuts.C
./compile ${which}.exe
rm myCuts.C myCuts_new.C
./MakeCondorFiles.csh ${which}
done

exit 0

